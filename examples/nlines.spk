#!/usr/bin/env spike-1

main(argv) {
    obj stream, line, i;
    
    stream = FileStream.open("README", "r");
    if (stream == null) {
        stderr.puts("Cannot open file!\n");
        return 1;
    }
    for (i = 1; (line = stream.gets(160)) != null; ++i) {
        stdout.printf("%04d: %s", i, line);
    }
    stream.close();
    return 0;
}
