#!/usr/bin/env python


import sys
from spike.compiler import compile


def main(argv):
    
    if len(argv) < 2:
        print >>sys.stderr, "Spike Compiler"
        print >>sys.stderr, "usage: %s FILE ..." % argv[0]
        return 1

    compile(argv[1:], sys.stdout)
    
    return 0


sys.exit(main(sys.argv))

